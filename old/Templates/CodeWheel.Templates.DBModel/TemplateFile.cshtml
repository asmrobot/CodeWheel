@model CodeWheel.Templates.DBModel.UIVO
@Model.ImportNameSpace

namespace @Model.NameSpace
{
    public partial class @(Model.ClassPre + Model.CurrentTable.UpperCamelName)
    {
        @foreach (var col in Model.CurrentTable.Columns)
        {
            <text>
            /// <summary>
            /// @(col.Comment)
            /// </summary>
            public @(col.FieldTypeName) @(col.UpperCamelColumnName)
            {
                get;
                set;
            }
            </text>

        }

    } 
}